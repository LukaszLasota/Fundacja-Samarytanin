{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
<header id="four">

{% include "components/menu.html.twig" %}

<section class="content-header">
    
    <div class="content-header-one">
        <div class="content-header-absolute">
            <img class="content-header-image" src="/img/statut/1.jpg" alt="Ręce starszej osoby">
            <h2 class="content-header-h2">statut</h2>
        </div>
    </div>

    <div class="content-header-two">
        <img src="/img/glowna-strona/2.jpg" alt="Tło z częścią loga fundacji">
        <div class="content-header-space"></div>
    </div>
    <div class="content-header-padding"></div>
</section>

<section class="content-mobile-subpage">
        <div class="one">
            <div class="one-text">
                <h2 class="one-h2">{{controller_name}}</h2>
            </div>
        </div>
        <div class="two">
            <img class="" src="/img/glowna-strona/2.jpg" alt="Tło z częścią loga fundacji">
            <img class="image-main" src="/img/statut/2.jpg" alt="Ręce starszej osoby">
        </div>
    </section>
</header>

</header>
<main>

<section class="statut-page">

<div class="statut-page-head">
    
<script src="//mozilla.github.io/pdf.js/build/pdf.js"></script>

<div class="center-pdf">
  
  <button id="prev" ><img src="/img/statut/3.jpg" alt="Poprzednia strona" title="Poprzednia strona"></button>
  <button id="next"><img src="/img/statut/3.jpg" alt="Następna strona" title="Następna strona"></button>
  &nbsp; &nbsp;
  <span>Page: <span id="page_num"></span> z <span id="page_count"></span></span>

<canvas id="the-canvas"></canvas>

<a class="more" href="/pdf/statut.pdf">POBIERZ STATUT</a>
        
</div>   

</div>






</section>

</main>
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('statut') }}

    <!-- Renders app.js & a webpack runtime.js file
    <script src="/build/runtime.js"></script>
    <script src="/build/app.js"></script> -->
{% endblock %}