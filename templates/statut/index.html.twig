{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
<header id="four">

{% include "components/menu.html.twig" %}

<section class="content-header">
    
    <div class="content-header-one">
        <div class="content-header-absolute">
            <img class="content-header-image" src="/img/statut/1.jpg" alt="Ręce starszej osoby">
            <h2 class="content-header-h2">statut</h2>
        </div>
    </div>

    <div class="content-header-two">
        <img src="/img/glowna-strona/2.jpg" alt="Tło z częścią loga fundacji">
